#composition-taxonomy = test_taxonomy.xml
#composition-services = test_serv.xml
fitness-weight1      = 0.25
fitness-weight2      = 0.25
fitness-weight3      = 0.25
fitness-weight4      = 0.25
overlap-enabled      = false
overlap-percentage   = 0.3
running-owls         = false
find-concepts        = true
ideal-path-length    = 0
ideal-num-atomic     = 0
